{
  "name": "boilerplato.github.io",
  "private": true,
  "version": "1.0.0",
  "description": "The Website https://boilerplato.com",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.dev.js --hot",
    "build": "webpack --config webpack.config.prod.js",
    "fetch-install-script": "curl -fsSL https://raw.githubusercontent.com/boilerplato/boilerplato/master/install.sh > install.sh",
    "deploy-to-gh-pages": "git checkout master && rm -rf static && cp -r ./dist/* . && npm run fetch-install-script && git add --all && git commit -m 'Make a build' && git push",
    "release": "git push origin develop && npm run build && npm run deploy-to-gh-pages && git checkout develop"
  },
  "author": "Rousan Ali <hello@rousan.io> (https://rousan.io/)",
  "license": "MIT",
  "homepage": "https://boilerplato.com/",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "antd": "^3.26.15",
    "babel-polyfill": "^6.26.0",
    "history": "^4.10.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.4",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}